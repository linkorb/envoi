{
  "name": "linkorb/envoi",
  "description": "Environment variables on steroids",
  "keywords": ["envoi"],
  "license": "MIT",
  "autoload":{
    "psr-4":{
      "Envoi\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4":{
      "EnvoiTest\\": "tests/"
    }
  },
  "require": {
    "php": "^7.1 || ^8.0",
    "symfony/dotenv": "^3.0|^4.0|^5.0|^6.0",
    "symfony/yaml": "^3.0|^4.0|^5.0|^6.0",
    "symfony/console": "^3.0|^4.0|^5.0|^6.0"
  },
  "bin": ["bin/envoi"],
  "require-dev": {
    "phpunit/phpunit": "^8"
  }
}
